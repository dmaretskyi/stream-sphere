{
  "name": "stream-sphere",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "STSP_DATA_DIR=~/.stsp next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "STSP_DATA_DIR=~/.stsp VITE_CJS_IGNORE_WARNING=true vitest --allowOnly",
    "init": "STSP_DATA_DIR=~/.stsp tsx src/bin/init.ts",
    "format": "prettier --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "format:fix": "prettier --write --ignore-path .gitignore . && eslint --fix ."
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.0.15",
    "async-parallel": "^1.2.3",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  }
}
